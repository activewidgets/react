{
  "name": "@activewidgets/react",
  "version": "3.4.4",
  "description": "Datagrid component for React",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "type": "module",
  "main": "./dist/main.js",
  "sideEffects": false,
  "exports": {
    ".": "./dist/main.js",
    "./index.js": "./dist/index.js",
    "./index.css": "./dist/index.css",
    "./style-inject": "./dist/style-inject.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node build.js",
    "examples": "npm run build && vite build",
    "prepack": "npm run build",
    "preview": "npm run examples && vite preview --open /",
    "start": "vite --open /",
    "test": "npm run examples && vitest run"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.4.4",
    "@activewidgets/frameworks": "3.4.4",
    "@activewidgets/ui": "*"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.16",
    "@activewidgets/options": "3.4.4",
    "@activewidgets/puppeteer": "0.1.3",
    "@testing-library/react": "^14",
    "react": "^18",
    "react-dom": "^18",
    "rollup-plugin-md": "^1",
    "vite": "^4",
    "vitest": "latest"
  },
  "overrides": {
    "jest": "npm:vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/activewidgets/react.git"
  },
  "keywords": [
    "react",
    "datagrid",
    "datatable",
    "virtualized",
    "scrolling",
    "grid",
    "table",
    "virtual",
    "data-grid",
    "data-table",
    "react-data-grid",
    "react-data-table",
    "activewidgets"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/react/issues"
  },
  "homepage": "https://activewidgets.com/react/data-grid/"
}
