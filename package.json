{
  "name": "@activewidgets/react",
  "version": "0.0.10",
  "description": "Datagrid component for React",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "*/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist/* && rollup -c",
    "output": "npm run build && rimraf out/* && build-examples",
    "prepack": "npm run build && rimraf out/*",
    "start": "serve-examples",
    "test": "npm run output && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "0.0.11",
    "@activewidgets/frameworks": "0.0.11"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.5",
    "@activewidgets/options": "0.0.11",
    "@activewidgets/puppeteer": "0.0.4",
    "@activewidgets/testing": "0.0.3",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@rollup/plugin-node-resolve": "^6",
    "@testing-library/react": "^9",
    "postcss-import": "^12",
    "react-dom": "^16",
    "rimraf": "^3",
    "rollup": "^1.20",
    "rollup-plugin-babel": "^4",
    "rollup-plugin-postcss": "^2",
    "rollup-plugin-sourcemaps": "^0.4",
    "rollup-plugin-terser": "^5",
    "style-inject": "^0.3"
  },
  "alias": {
    "@activewidgets/react": "./"
  },
  "jest": {
    "projects": [
      {
        "displayName": "Unit",
        "preset": "@activewidgets/testing",
        "moduleNameMapper": {
          "@activewidgets/components": "<rootDir>/js",
          "@activewidgets/testing": "<rootDir>/test/adapter/react.js"
        }
      },
      {
        "displayName": "Visual",
        "preset": "@activewidgets/puppeteer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/react.git"
  },
  "keywords": [
    "react"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/react/issues"
  },
  "homepage": "https://github.com/activewidgets/react#readme"
}
